{
  "name": "daniel-monroy/sat-estado-cfdi",
  "description": "Paquete interno Laravel para consultar estado CFDI (SAT)",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0",
    "phpcfdi/sat-estado-cfdi": "^2.0",
    "phpcfdi/cfdi-expresiones": "^v3.3.0",
    "guzzlehttp/guzzle": "^7.9",
    "nyholm/psr7": "^1.8",
    "php-http/guzzle7-adapter": "^1.0",
    "ext-dom": "*"
  },
  "autoload": {
    "psr-4": {
      "DanielMonroy\\SatEstadoCfdi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DanielMonroy\\SatEstadoCfdi\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DanielMonroy\\SatEstadoCfdi\\SatEstadoCfdiServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint": "vendor/bin/pint",
    "test": "vendor/bin/pest"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
